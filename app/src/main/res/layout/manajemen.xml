<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="118dp"
        android:layout_height="125dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="276dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logoo_1" />

    <LinearLayout
        android:id="@+id/baris2"
        android:layout_width="270dp"
        android:layout_height="100dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="4dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bunga Laundry"
            android:textStyle="bold"
            android:textColor="#5AA9E6"
            android:textSize="22dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Alamat"
            android:textStyle="bold"
            android:textColor="#5AA9E6"
            android:textSize="10dp"/>

        <TextView
            android:id="@+id/textView9"
            android:layout_width="214dp"
            android:layout_height="25dp"
            android:text="Jl. Candi Panggung No.22, Mojolangu, Kec. Lowokwaru, Kota Malang, Jawa Timur"
            android:textColor="#7FC8F8"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Buka"
            android:textStyle="bold"
            android:layout_marginTop="5dp"
            android:textColor="#5AA9E6"
            android:textSize="10dp"/>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="07:00 - 20:00"
            android:textColor="#7FC8F8"
            android:textSize="10dp"/>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/baris12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/baris2"
        tools:layout_editor_absoluteX="17dp">

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:text="Pendapatan Hari Ini"
            android:textColor="#7FC8F8" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:text="Pengeluaran Hari Ini"
            android:textColor="#7FC8F8" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/baris13"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/baris12"
        tools:layout_editor_absoluteX="16dp">

        <EditText
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="149dp"
            android:layout_height="20dp"
            android:layout_marginLeft="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="" />

        <EditText
            android:id="@+id/editTextTextPersonName4"
            android:layout_width="149dp"
            android:layout_height="20dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="" />

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:layout_marginTop="260dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/refresh" />


    <LinearLayout
        android:id="@+id/baris3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="316dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="-4dp">

        <ImageButton
            android:id="@+id/imageView15"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="40dp"
            android:backgroundTint="@color/white"
            app:srcCompat="@drawable/kelola_laundry" />

        <ImageButton
            android:id="@+id/imageView16"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginLeft="60dp"
            android:backgroundTint="@color/white"
            app:srcCompat="@drawable/riwayat_transaksi" />

        <ImageButton
            android:id="@+id/imageView17"
            android:layout_width="71dp"
            android:layout_height="65dp"
            android:layout_marginLeft="60dp"
            android:backgroundTint="@color/white"
            app:srcCompat="@drawable/kas_laundry" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/baris4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/baris3">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:text="Kelola Laundry"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:text="Riwayat Transaksi"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:text="Kas Transaksi"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.894"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.578" />

    </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>