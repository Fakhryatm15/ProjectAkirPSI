<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/backKL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="370dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />

    <TextView
        android:id="@+id/kaslaundry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="105dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="105dp"
        android:text="Kas Laundry"
        android:textColor="#48B8F4"
        android:textStyle="bold"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <LinearLayout
        android:id="@+id/barisKL0"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="15dp"
        android:background="#FFFFFF"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kaslaundry">


        <ImageView
            android:id="@+id/show2KL"
            android:layout_width="53dp"
            android:layout_height="42dp"
            android:layout_marginLeft="137dp"
            android:layout_marginTop="-5dp"
            app:srcCompat="@drawable/show__1_" />
        <ImageView
            android:id="@+id/show3KL"
            android:layout_width="53dp"
            android:layout_height="42dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="-5dp"
            app:srcCompat="@drawable/show__2_" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="-5dp"
            android:layout_marginLeft="0dp"
            android:inputType="text"
            android:hint="cari"
            android:textSize="12sp"/>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/barisKL1"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="15dp"
        android:background="#F4F4F4"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barisKL0">

        <ImageView
            android:id="@+id/hijau"
            android:layout_width="29dp"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="-9dp"
            app:srcCompat="@drawable/rectangle_44" />

        <LinearLayout
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textPemasukan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="Pemasukan"
                android:textColor="#48B8F4"
                android:textSize="12sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/textTransaksi"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="#B5B5B5"
                android:textSize="12sp"
                android:text="Transaksi"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textJumlah"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="Rp 200.000"
                android:textColor="#B5B5B5"
                android:textSize="12sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/textKet"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="#B5B5B5"
                android:layout_marginTop="3dp"
                android:textSize="9sp"
                android:text="Transaksi Sehari (10/09/2022)"/>
        </LinearLayout>
        <ImageView
            android:id="@+id/delete1"
            android:layout_width="29dp"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="-9dp"
            app:srcCompat="@drawable/delete" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/barisKL2"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="15dp"
        android:background="#F4F4F4"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barisKL1">

        <ImageView
            android:id="@+id/merah1"
            android:layout_width="29dp"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="-9dp"
            app:srcCompat="@drawable/rectangle_44__1_" />

        <LinearLayout
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textPengeluaran"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="Pengeluaran"
                android:textColor="#48B8F4"
                android:textSize="12sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/textPembelian"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="#B5B5B5"
                android:textSize="12sp"
                android:text="Pembelian"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textJumlah2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="-Rp 15.000"
                android:textColor="#B5B5B5"
                android:textSize="12sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/textKet2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="#B5B5B5"
                android:layout_marginTop="3dp"
                android:textSize="9sp"
                android:text="Beli Pewangi Setrika"/>
        </LinearLayout>
        <ImageView
            android:id="@+id/delete2"
            android:layout_width="29dp"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="-9dp"
            app:srcCompat="@drawable/delete" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/barisKL3"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="15dp"
        android:background="#F4F4F4"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barisKL2">

        <ImageView
            android:id="@+id/merah2"
            android:layout_width="29dp"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="-9dp"
            app:srcCompat="@drawable/rectangle_44__1_" />

        <LinearLayout
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textPengeluaran2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="Pemasukan"
                android:textColor="#48B8F4"
                android:textSize="12sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/textTransaksi2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="#B5B5B5"
                android:textSize="12sp"
                android:text="Transaksi"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textJumlah3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="-Rp 120.000"
                android:textColor="#B5B5B5"
                android:textSize="12sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/textKet3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textColor="#B5B5B5"
                android:layout_marginTop="3dp"
                android:textSize="9sp"
                android:text="Bayar Listrik"/>
        </LinearLayout>
        <ImageView
            android:id="@+id/delete3"
            android:layout_width="29dp"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="-9dp"
            app:srcCompat="@drawable/delete" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>